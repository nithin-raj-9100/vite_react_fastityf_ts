{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm -w run vercel-build",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/[[...route]].js": {
      "runtime": "nodejs18.x",
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[[...route]]"
    },
    {
      "source": "/((?!api/|_next/|assets/|static/|favicon.ico|vite.svg|sw.js|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
