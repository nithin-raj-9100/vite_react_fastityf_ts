{
  "version": 2,
  "buildCommand": "pnpm run vercel-build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/**/*": {
      "runtime": "nodejs18.x"
    }
  }
}
