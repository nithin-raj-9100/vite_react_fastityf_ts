{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm -w run vercel-build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
