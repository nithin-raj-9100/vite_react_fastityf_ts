{
  "version": 2,
  "installCommand": "pnpm install",
  "buildCommand": "pnpm -w run vercel-build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[[...route]].js"
    },
    {
      "source": "/((?!api/|_next/|assets/|static/|favicon.ico|vite.svg|sw.js|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
